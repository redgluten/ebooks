    <dc:title>Nouvelles - Volume I</dc:title>
    <dc:creator opf:file-as="Wells, Herbert Georges" opf:role="aut">Herbert Georges Wells</dc:creator>
    <dc:language>fr</dc:language>
    <dc:subject>Fantastique &amp; SF</dc:subject>
