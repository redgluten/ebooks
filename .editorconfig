# EditorConfig is awesome: http://EditorConfig.org

# top-most EditorConfig file
root = true

# Every files
[*]
indent_style = spaces
end_of_line = lf
charset = utf-8
trim_trailing_whitespace = true
insert_final_newline = true

[*.md]
trim_trailing_whitespace = false

[*.css]
indent_style = tabs

[*.scss]
indent_style = tabs
